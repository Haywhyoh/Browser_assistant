{
  "name": "Browser Assistant",
  "version": "1.0",
  "manifest_version": 3,
  "description": "An extension to make your browsing the internet fun and easy",
  "permissions": ["storage"],
  "background": {
    "service_worker": "../src/service.js"
  },
  "action": {
    "default_title": "Browser Assistant",
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon_32.png",
      "48": "icon_32.png",
      "32": "icon_32.png"
    }
  }
}
